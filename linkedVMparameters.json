{
  "$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
      "networkSecurityGroupName": {
          "value": "comp1-nsg"
      },
      "diskType": {
          "value": "Standard_LRS"
      },
      "vmName": {
          "value": "kristianVM"
      },
      "vmSize": {
          "value": "Standard_A2"
      },
      "imageSKU": {
          "value": "2016-Datacenter"
      },
      "adminUsername": {
          "value": "kristian"
      },
      "adminPassword": {
        "reference":  {
            "keyVault":  {
              "id":  "/subscriptions/ae8ab787-c66f-4733-aed2-228ab3e8523b/resourceGroups/KristianResourceGroup/providers/Microsoft.KeyVault/vaults/kristianVault"
              },
            "secretName":  "kristianVmPassword"
         }
      },
      "networkSecurityGroupRules": {
        "value": [
            {
                "name": "HTTP",
                "properties": {
                    "priority": 300,
                    "protocol": "TCP",
                    "access": "Allow",
                    "direction": "Inbound",
                    "sourceAddressPrefix": "*",
                    "sourcePortRange": "*",
                    "destinationAddressPrefix": "*",
                    "destinationPortRange": "80"
                }
            },
            {
                "name": "TaskIn",
                "properties": {
                    "priority": 410,
                    "protocol": "TCP",
                    "access": "Allow",
                    "direction": "Inbound",
                    "sourceAddressPrefix": "*",
                    "sourcePortRange": "*",
                    "destinationAddressPrefix": "*",
                    "destinationPortRange": "8080"
                }
            },
            {
                "name": "RDPIn",
                "properties": {
                    "priority": 420,
                    "protocol": "TCP",
                    "access": "Allow",
                    "direction": "Inbound",
                    "sourceAddressPrefix": "*",
                    "sourcePortRange": "*",
                    "destinationAddressPrefix": "*",
                    "destinationPortRange": "3389"
                }
            },
            {
                "name": "RDPOut",
                "properties": {
                    "priority": 430,
                    "protocol": "TCP",
                    "access": "Allow",
                    "direction": "Outbound",
                    "sourceAddressPrefix": "*",
                    "sourcePortRange": "3389",
                    "destinationAddressPrefix": "*",
                    "destinationPortRange": "*"
                }
            }
        ]
      },
     
      "virtualNetworkId": {
        "value": "/subscriptions/ae8ab787-c66f-4733-aed2-228ab3e8523b/resourceGroups/KristianResourceGroup/providers/Microsoft.Network/virtualNetworks/KristianShevandoVNET"
